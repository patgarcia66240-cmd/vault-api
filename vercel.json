{
  "installCommand": "pnpm install",
  "buildCommand": "pnpm --filter web build && cp -r apps/web/dist ./dist",
  "outputDirectory": "dist",
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9",
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.py"
    }
  ]
}
